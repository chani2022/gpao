 <style>
 .notification_message{
     position: absolute;
    display: inline-block;
    height: 15px;
    width: 15px;
    line-height: 0px;
    text-align: center;
    color:#fff ;
    -webkit-border-radius: 100%;
    -moz-border-radius: 100%;
    border-radius: 100%;
    font-size: 12px;
 
 }
 .tous_message{
     font-size: 12px;
    display: block;
    text-align: center;
    font-size: 14px;
    padding: 24px 0;
    color: #3868cd;
 }
 .quantity_suite{
     background-color: #ed657d;
    color: #f2f2f2;
    text-align: center;
    border: 1px solid transparent;
    border-radius: 4em;
    box-shadow: 5px 5px 4px rgba(0, 0, 0, .2);
 }

 .account-dropdown .info {
    padding: 25px;
    border-top: 1px solid #f5f5f5;
    border-bottom: 1px solid #f2f2f2;
}
.account-dropdown .info .content {
    margin-left: 65px;
    padding: 11px 0;
    padding-left: 12px;
}
.account-item--style2 .account-dropdown .info .image {
    -webkit-border-radius: 100%;
    -moz-border-radius: 100%;
    border-radius: 100%;
}
 </style>   

