<body>
 

    {# header #}
    <div class="header-top-area">
        <div class="container">
            <div class="row">
                <div class="col-lg-2 col-md-2 col-sm-12 col-xs-12">
                    <div class="logo-area">
                        <a href="#"><img src="{{ absolute_url(asset('images/logo-eline-small-archives.png')) }}" alt="" /></a>
                    </div>
                </div>
                
                <div class="col-lg-6 col-md-2 col-sm-12 col-xs-12">
                    <div class="logo-area">
                        <form action="{{ path('transmission_fast_search') }}" method="post" class="form-inline">
                            <div class="form-group">
                                {% set varname = app.session.get('keys_rapide') %}
                                 <input name="fast_search" value="{{varname}}"   type="text" class="form-control" placeholder="Recherche rapide...">
                              </div>
                              <div class="form-group">
                                 
                                  &nbsp;<input name="check_text" class="form-check-input" type="checkbox" value="">
                              </div>
                            <button type="submit" class="btn btn-primary">Rechercher</button>
                          </form>
                    </div>
                </div>
                <div class="col-lg-4 col-md-8 col-sm-12 col-xs-12">
                    <div class="header-top-menu">
                    <ul class="nav navbar-nav notika-top-nav">
                            <li class="nav-item dropdown">
                                        <a href="#" data-toggle="dropdown" role="button" aria-expanded="false" class="nav-link dropdown-toggle">
                                            <span>
                                                {% if app.user==null %}
                                                    {{ logout_path() }}
                                                {% endif %}
                                                <img style="border-radius: 100%;"  width="20" height="20" src="http://192.168.8.3/public/user_photo/{{ app.user.userdetails['photo'] }}"
                                                                                /> {{ app.user.userdetails['login']|capitalize  }}
                                                   
                                                
                                            </span>
                                        </a>
                                        <div role="menu" class="dropdown-menu message-dd animated zoomIn">
                                                <div class="hd-message-sn">
                                                    <div class="hd-mg-ctn">
                                                        <a href="{{ path('logout') }}">
                                                            <h3><i class="fas fa-power-off"></i> Quitter</h3>
                                                        </a>
                                                    </div>
                                                </div>
                                        </div>
                            </li>
                        </ul>
                        <ul class="nav navbar-nav notika-top-nav">
                            <li class="nav-item dropdown" style="right:20px">
                                <a href="#" data-toggle="dropdown" role="button" aria-expanded="false" class="nav-link ">
                                    <span><i class="zmdi zmdi-comment-more"></i></span>
                                    {% if is_granted("ROLE_SUPER_ADMIN") and app.request.get('_route') == 'dossier_extra_general' or app.request.get('_route') == 'dossier_suivi_extra_heure_manquant' or app.request.get('_route') == 'dossier_suivi_extra_extra_effectuez' or app.request.get('_route') == 'dossier_statistique_extra_et_production' %}
                                            <span class="notification_message"></span>
                                    {% else %}
                                           <span class="notification_message">{{ render(controller('App\\Controller\\TransmissionController::getMyUnreadMessage')) }}</span>
                                    {% endif %}
                                </a>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </div>
    {# end header #}

    {{ render(controller('App\\Controller\\MainController::getMenus', {activeRoutes:ROUTES_ACTIVE, newMenus:1})) }}
    
